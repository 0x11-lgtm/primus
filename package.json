{
  "name": "primus",
  "version": "0.0.0",
  "description": "Primus is a simple abstraction around real-time frameworks. It allows you to easily switch between different frameworks without any code changes.",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=testing ./node_modules/.bin/mocha $(find test -name '*.test.js')",
    "browser": "./node_modules/.bin/testling ."
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/3rd-Eden/primus.git"
  },
  "keywords": [
    "primus",
    "real-time",
    "realtime",
    "websocket",
    "ws",
    "engine.io",
    "socket.io",
    "transformer"
  ],
  "author": "Arnout Kazemier",
  "license": "MIT",
  "dependencies": {
    "extendable": "0.0.x",
    "load": "0.1.x"
  },
  "testling": {
    "harness": "mocha-bdd",
    "files": "test/*.browser.js",
    "browsers": {
      "ie": [ 6, 7, 8 ],
      "ff": [ 19 ],
      "safari": [ 5 ],
      "opera": [ 12 ],
      "chrome": [ 25 ]
    }
  },
  "devDependencies": {
    "browserchannel": "1.0.x",
    "chai": "1.7.x",
    "engine.io": "0.6.x",
    "engine.io-client": "0.6.x",
    "jsonh": "0.0.x",
    "mocha": "1.11.x",
    "pre-commit": "0.0.x",
    "socket.io": "0.9.x",
    "socket.io-client": "0.9.x",
    "testling": "*",
    "ws": "0.4.x"
  }
}
